  <Desc>NVL is the Oracle equivelent of IFNULL
If the value is null, replace it with a predifined replacement value
</Desc>
  <Auther name="" id="" />
  <Language>Java</Language>
  <FunctionBody>if(value==null)
	return  replacement;
else
	return value;
</FunctionBody>
  <IsSyntaxValid>true</IsSyntaxValid>
  <IsRootFunction>false</IsRootFunction>
  <InputParams>
    <Input name="value" id="fi.ORACLE.NVL.0" index="0" dataType="String">
      <Comment>Value that might be null</Comment>
    </Input>
    <Input name="replacement" id="fi.ORACLE.NVL.1" index="1" dataType="String">
      <Comment>Replace with this when value is null</Comment>
    </Input>
  </InputParams>
  <OutputParams>
    <Output name="retVal" id="fo.ORACLE.NVL.0" index="0" dataType="String">
      <Comment />
    </Output>
  </OutputParams>
</KFunction>